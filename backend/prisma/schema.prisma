// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Estabelecimento {
  id        Int      @id @default(autoincrement())
  nome      String
  endereco  String
  bairro    String
  segmento  String
  telefone  String
  whatsapp  String?
  imagem    String?
  createdAt DateTime @default(now())
}
